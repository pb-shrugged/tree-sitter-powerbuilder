{
  "name": "@pb-shrugged/tree-sitter-powerbuilder",
  "version": "2.0.1",
  "description": "Powerbuilder grammar for tree-sitter",
  "repository": "https://github.com/pb-shrugged/tree-sitter-powerbuilder",
  "license": "MIT",
  "author": {
    "name": "Jose Cagnini"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "powerbuilder"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.1",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "eslint": "^9.35.0",
    "eslint-config-treesitter": "^1.0.2",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.7"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "lint": "eslint grammar.js",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "ts:start": "npm run ts:generate && npm run ts:run",
    "ts:generate": "tree-sitter generate",
    "ts:run": "tree-sitter build --wasm && tree-sitter playground -q",
    "ts:test": "tree-sitter test",
    "ts:parse:playground": "npm run ts:generate && tree-sitter parse ./examples/playground.srf",
    "lint:fix": "eslint grammar.js --fix"
  }
}
