{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "powerbuilder",
      "camelcase": "PowerBuilder",
      "title": "Powerbuilder",
      "scope": "source.powerbuilder",
      "file-types": [
        "srs",
        "srf",
        "srw",
        "sru",
        "sra",
        "srm"
      ],
      "injection-regex": "^powerbuilder$",
      "class-name": "TreeSitterPowerbuilder"
    }
  ],
  "metadata": {
    "version": "2.0.1",
    "license": "MIT",
    "description": "Powerbuilder grammar for tree-sitter",
    "authors": [
      {
        "name": "Jose Cagnini"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-powerbuilder"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}